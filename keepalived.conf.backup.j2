vrrp_instance VI_1 {
    state BACKUP
    interface ens192
    virtual_router_id 1
    priority 100
    advert_int 1
    nopreempt
    authentication {
        auth_type PASS
        auth_pass {{ hostvars[item]['keepalived_secret']['stdout'] }}
    }
    virtual_ipaddress {
        {{ api_ip }}
    }
}
